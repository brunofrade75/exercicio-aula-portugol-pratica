programa {
  funcao inicio() {
    cadeia nomeCidadeMoradia, nomeCidadeEstudo

    escreva ("\nDigite o nome da sua cidade de nascimento:")
    leia(nomeCidadeMoradia)

    escreva ("\n\nDigite a cidade onde voce estuda:")
    leia (nomeCidadeEstudo)

    escreva("\nMoradia: " + nomeCidadeMoradia + " Estudo:" + nomeCidadeEstudo)
  }
}
